[mysql]

[mysqld]
sql_mode="STRICT_TRANS_TABLES"
character-set-server=utf8
default-authentication-plugin=mysql_native_password
group_concat_max_len=16777216
max_allowed_packet=33554432
innodb_buffer_pool_size=4G